@rem ========================================================================
@rem   ユーザ情報Dump用スクリプト
@rem   Version:1.0
@rem ========================================================================
@rem                   吉積情報株式会社
@rem ========================================================================

call ..\common\env\csvde_common_env.bat

@rem CSVファイル名-----------------------------------------------------------
set CSV_FILE_NAME=activeDirectory_1.csv

@rem BASE_DN_NAME------------------------------------------------------------
rem set BASE_DN_NAME="OU=Users,OU=sigma,DC=sigma,DC=local"
set BASE_DN_NAME="OU=app.yoshidumi,OU=sigma,DC=SHISUITEN,DC=LOCAL"

@rem Filter------------------------------------------------------------------
set FILTER="(objectClass=user)"

@rem ActiveDirectory Dumpパラメータ------------------------------------------
set OUT_FIELD=userPrincipalName
set OUT_FIELD=%OUT_FIELD%,whenChanged
set OUT_FIELD=%OUT_FIELD%,displayName
set OUT_FIELD=%OUT_FIELD%,sn
set OUT_FIELD=%OUT_FIELD%,givenName
set OUT_FIELD=%OUT_FIELD%,telephoneNumber
set OUT_FIELD=%OUT_FIELD%,mail
@rem set OUT_FIELD=%OUT_FIELD%,st
@rem set OUT_FIELD=%OUT_FIELD%,l
@rem set OUT_FIELD=%OUT_FIELD%,streetAddress
@rem set OUT_FIELD=%OUT_FIELD%,homePhone
set OUT_FIELD=%OUT_FIELD%,mobile
set OUT_FIELD=%OUT_FIELD%,facsimileTelephoneNumber
@rem set OUT_FIELD=%OUT_FIELD%,department
@rem set OUT_FIELD=%OUT_FIELD%,title
@rem set OUT_FIELD=%OUT_FIELD%,pager
set OUT_FIELD=%OUT_FIELD%,description
set OUT_FIELD=%OUT_FIELD%,physicalDeliveryOfficeName
set OUT_FIELD=%OUT_FIELD%,ipPhone

@rem 共通変数のインポート----------------------------------------------------
call csvde_invoke %CSV_FILE_NAME% %BASE_DN_NAME% %FILTER%